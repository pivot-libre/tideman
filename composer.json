{
    "name": "pivot-libre/tideman",
    "description": "Implementation of the Tideman ranked pair algorithm",
    "type": "library",
    "require": {
        "psr/log": "^1.0.2",
        "clue/graph": "0.9.0",
        "graphp/algorithms": "0.8.1"
    },
    "require-dev": {
        "cadre/sniffs": "^0.1.0",
        "monolog/monolog": "^1.23.0",
        "pds/skeleton": "^1.0.0",
        "phing/phing": "^2.16.0",
        "phpstan/phpstan": "^0.6.3",
        "phpunit/phpunit": "^6.2.3",
        "satooshi/php-coveralls": "^1.0.1",
        "squizlabs/php_codesniffer": "^3.0.1"
    },
    "autoload": {
        "psr-4": {
            "PivotLibre\\Tideman\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PivotLibre\\Tideman\\": "tests/",
            "PHP_CodeSniffer\\": "vendor/squizlabs/php_codesniffer/src/"
        }
    },
    "license": "Apache-2.0",
    "minimum-stability": "dev"
